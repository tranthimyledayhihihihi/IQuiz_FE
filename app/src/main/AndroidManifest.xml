        </activity>

        <activity android:name=".feature.result.ui.ResultActivity" />
        <activity android:name=".feature.result.ui.DailyRewardActivity" />
        <activity android:name=".feature.result.ui.AchievementActivity" />
        <activity android:name=".feature.result.ui.StatsActivity" />
        <activity android:name=".feature.result.ui.StreakActivity" />

    </application>
</manifest>